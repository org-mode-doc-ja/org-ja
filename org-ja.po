#. type: Plain text
#: org.texi:12732
msgid "It is possible to call functions from the command line.  This shell script calls @code{org-babel-tangle} on every one of its arguments."
msgstr ""

#. type: Plain text
#: org.texi:12734
msgid "Be sure to adjust the paths to fit your system."
msgstr ""

#. type: example
#: org.texi:12744
#, no-wrap
msgid ""
"#!/bin/sh\n"
"# -*- mode: shell-script -*-\n"
"#\n"
"# tangle files with org-mode\n"
"#\n"
"DIR=`pwd`\n"
"FILES=\"\"\n"
"ORGINSTALL=\"~/src/org/lisp/org-install.el\"\n"
"\n"
msgstr ""

#. type: example
#: org.texi:12749
#, no-wrap
msgid ""
"# wrap each argument in the code required to call tangle on it\n"
"for i in $@@; do\n"
"    FILES=\"$FILES \\\"$i\\\"\"\n"
"done\n"
"\n"
msgstr ""

#. type: example
#: org.texi:12759
#, no-wrap
msgid ""
"emacs -Q --batch -l $ORGINSTALL \\\n"
"--eval \"(progn\n"
"(add-to-list 'load-path (expand-file-name \\\"~/src/org/lisp/\\\"))\n"
"(add-to-list 'load-path (expand-file-name \\\"~/src/org/contrib/lisp/\\\"))\n"
"(require 'org)(require 'org-exp)(require 'ob)(require 'ob-tangle)\n"
"(mapc (lambda (file)\n"
"       (find-file (expand-file-name file \\\"$DIR\\\"))\n"
"       (org-babel-tangle)\n"
"       (kill-buffer)) '($FILES)))\" 2>&1 |grep tangled\n"
msgstr ""

