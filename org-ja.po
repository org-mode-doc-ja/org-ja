#: org.texi:13136
#: org.texi:13359
#, no-wrap
msgid "org-hide-leading-stars"
msgstr ""

#. type: table
#: org.texi:13142
msgid "Here are the options for hiding leading stars in outline headings, and for indenting outlines.  The corresponding variables are @code{org-hide-leading-stars} and @code{org-odd-levels-only}, both with a default setting @code{nil} (meaning @code{showstars} and @code{oddeven})."
msgstr ""

#. type: cindex
#: org.texi:13142
#, no-wrap
msgid "@code{hidestars}, STARTUP keyword"
msgstr ""

#. type: cindex
#: org.texi:13143
#, no-wrap
msgid "@code{showstars}, STARTUP keyword"
msgstr ""

#. type: cindex
#: org.texi:13144
#, no-wrap
msgid "@code{odd}, STARTUP keyword"
msgstr ""

#. type: cindex
#: org.texi:13145
#, no-wrap
msgid "@code{even}, STARTUP keyword"
msgstr ""

#. type: example
#: org.texi:13153
#, no-wrap
msgid ""
"hidestars  @r{make all but one of the stars starting a headline invisible.}\n"
"showstars  @r{show all stars starting a headline}\n"
"indent     @r{virtual indentation according to outline level}\n"
"noindent   @r{no virtual indentation according to outline level}\n"
"odd        @r{allow only odd outline levels (1,3,...)}\n"
"oddeven    @r{allow all outline levels}\n"
msgstr ""

