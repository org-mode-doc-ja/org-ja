msgstr ""

#. type: table
#: org.texi:6542
msgid "Select a file and move it into the task's attachment directory.  The file will be copied, moved, or linked, depending on @code{org-attach-method}.  Note that hard links are not supported on all systems."
msgstr ""

#. type: kindex
#: org.texi:6543
#, no-wrap
msgid "C-c C-a c"
msgstr ""

#. type: kindex
#: org.texi:6544
#, no-wrap
msgid "C-c C-a m"
msgstr ""

#. type: kindex
#: org.texi:6545
#, no-wrap
msgid "C-c C-a l"
msgstr ""

#. type: item
#: org.texi:6546
#, no-wrap
msgid "c/m/l"
msgstr ""

#. type: table
#: org.texi:6549
msgid "Attach a file using the copy/move/link method.  Note that hard links are not supported on all systems."
msgstr ""

#. type: table
#: org.texi:6552
msgid "@orgcmdtkc{n,C-c C-a n,org-attach-new} Create a new attachment as an Emacs buffer."
msgstr ""

#. type: table
#: org.texi:6556
msgid "@orgcmdtkc{z,C-c C-a z,org-attach-sync} Synchronize the current task with its attachment directory, in case you added attachments yourself."
msgstr ""

#. type: table
#: org.texi:6558
