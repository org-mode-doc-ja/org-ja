#: org.texi:12744
#, no-wrap
msgid ""
"#!/bin/sh\n"
"# -*- mode: shell-script -*-\n"
"#\n"
"# tangle files with org-mode\n"
"#\n"
"DIR=`pwd`\n"
"FILES=\"\"\n"
"ORGINSTALL=\"~/src/org/lisp/org-install.el\"\n"
"\n"
msgstr ""
