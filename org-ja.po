#, no-wrap
msgid ""
"emacs -batch -l ~/.emacs                                      \\\n"
"   -eval '(org-batch-agenda \"a\"                               \\\n"
"            org-agenda-span month                             \\\n"
"            org-agenda-include-diary nil                      \\\n"
"            org-agenda-files (quote (\"~/org/project.org\")))'  \\\n"
"   | lpr\n"
msgstr ""

#+glossary: agenda ( アジェンダ ) / diary ( 日記 ) / file ( ファイル ) / org ( Org-mode )
#. type: Plain text
#: org.texi:14385
msgid "which will produce a 30-day agenda, fully restricted to the Org file @file{~/org/projects.org}, not even including the diary."
msgstr ""

#+glossary: agenda ( アジェンダ ) / command ( コマンド ) / data ( データ ) / field ( フィールド ) / item ( アイテム|項目 ) / line ( 行|ライン|線 ) / list ( リスト ) / output ( アウトプット ) / value ( 値 )
#. type: Plain text
#: org.texi:14391
msgid "If you want to process the agenda data in more sophisticated ways, you can use the command @code{org-batch-agenda-csv} to get a comma-separated list of values for each agenda item.  Each line in the output will contain a number of fields separated by commas.  The fields in a line are:"
msgstr ""

#+glossary: agenda ( アジェンダ ) / block ( ブロック ) / category ( カテゴリー ) / date ( 日付 ) / deadline ( デッドライン ) / diary ( 日記 ) / entry ( エントリー|文|入力 ) / headline ( 見出し ) / keyword ( キーワード ) / planning ( 計画 ) / priority ( 優先順位|プライオリティ ) / scheduled ( スケジューリングされる ) / tag ( タグ ) / time ( 時刻|時間|回 ) / timestamp ( タイムスタンプ ) / type ( タイプ|入力する )
#. type: example
#: org.texi:14413
#, no-wrap
msgid ""
"category     @r{The category of the item}\n"
"head         @r{The headline, without TODO keyword, TAGS and PRIORITY}\n"
"type         @r{The type of the agenda entry, can be}\n"
"                todo               @r{selected in TODO match}\n"
"                tagsmatch          @r{selected in tags match}\n"
"                diary              @r{imported from diary}\n"
"                deadline           @r{a deadline}\n"
"                scheduled          @r{scheduled}\n"
"                timestamp          @r{appointment, selected by timestamp}\n"
"                closed             @r{entry was closed on date}\n"
"                upcoming-deadline  @r{warning about nearing deadline}\n"
"                past-scheduled     @r{forwarded scheduled item}\n"
"                block              @r{entry has date block including date}\n"
"todo         @r{The TODO keyword, if any}\n"
"tags         @r{All tags including inherited ones, separated by colons}\n"
"date         @r{The relevant date, like 2007-2-14}\n"
"time         @r{The time, like 15:00-16:50}\n"
"extra        @r{String with extra planning info}\n"
"priority-l   @r{The priority letter if any was given}\n"
"priority-n   @r{The computed numerical priority}\n"
msgstr ""

#+glossary: date ( 日付 ) / item ( アイテム|項目 ) / time ( 時刻|時間|回 ) / timestamp ( タイムスタンプ )
#. type: Plain text
#: org.texi:14418
msgid "Time and date will only be given if a timestamp (or deadline/scheduled)  led to the selection of the item."
msgstr ""

#+glossary: example ( 例 ) / item ( アイテム|項目 ) / list ( リスト )
#. type: Plain text
#: org.texi:14422
msgid "A CSV list like this is very easy to use in a post-processing script.  For example, here is a Perl program that gets the TODO list from Emacs/Org and prints all the items, preceded by a checkbox:"
msgstr ""

#. type: example
#: org.texi:14425
#, no-wrap
msgid ""
"#!/usr/bin/perl\n"
"\n"
msgstr ""

#+glossary: command ( コマンド )
#. type: example
#: org.texi:14428
#, no-wrap
msgid ""
"# define the Emacs command to run\n"
"$cmd = \"emacs -batch -l ~/.emacs -eval '(org-batch-agenda-csv \\\"t\\\")'\";\n"
"\n"
msgstr ""

#+glossary: capture ( キャプチャ )
#. type: example
#: org.texi:14431
#, no-wrap
msgid ""
"# run it and capture the output\n"
"$agenda = qx@{$cmd 2>/dev/null@};\n"
"\n"
msgstr ""

#. type: example
#: org.texi:14440
#, no-wrap
msgid ""
"# loop over all lines\n"
"foreach $line (split(/\\n"
"/,$agenda)) @{\n"
"  # get the individual values\n"
"  ($category,$head,$type,$todo,$tags,$date,$time,$extra,\n"
"   $priority_l,$priority_n) = split(/,/,$line);\n"
"  # process and print\n"
"  print \"[ ] $head\\n"
"\";\n"
"@}\n"
msgstr ""

