msgstr "あなたのシステムに合わせてパスを設定するように注意してください。"

# [Org7.5-3474::@]
# glossary: file ( ファイル ) / tangle ( 抽出する )
#. type: example
#: org.texi:12779
#, no-wrap
msgid ""
"#!/bin/sh\n"
"# -*- mode: shell-script -*-\n"
"#\n"
"# tangle files with org-mode\n"
"#\n"
"DIR=`pwd`\n"
"FILES=\"\"\n"
"ORGINSTALL=\"~/src/org/lisp/org-install.el\"\n"
"\n"
msgstr ""
"#!/bin/sh\n"
"# -*- mode: shell-script -*-\n"
"#\n"
"# org-mode でファイルを抽出する\n"
"#\n"
"DIR=`pwd`\n"
"FILES=\"\"\n"
"ORGINSTALL=\"~/src/org/lisp/org-install.el\"\n"
"\n"

# [Org7.5-3475::@]
# glossary: argument ( 引数 ) / code ( コード ) / tangle ( 抽出する )
#. type: example
#: org.texi:12784
#, no-wrap
msgid ""
"# wrap each argument in the code required to call tangle on it\n"
"for i in $@@; do\n"
"    FILES=\"$FILES \\\"$i\\\"\"\n"
"done\n"
"\n"
msgstr ""
"# tangleする必要があるそれぞれの引数をクォーテーションで囲む\n"
"for i in $@@; do\n"
"    FILES=\"$FILES \\\"$i\\\"\"\n"
"done\n"
"\n"

# [Org7.5-3476::@]
# glossary: file ( ファイル )
#. type: example
#: org.texi:12794
#, no-wrap
msgid ""
"emacs -Q --batch -l $ORGINSTALL \\\n"
"--eval \"(progn\n"
"(add-to-list 'load-path (expand-file-name \\\"~/src/org/lisp/\\\"))\n"
"(add-to-list 'load-path (expand-file-name \\\"~/src/org/contrib/lisp/\\\"))\n"
"(require 'org)(require 'org-exp)(require 'ob)(require 'ob-tangle)\n"
"(mapc (lambda (file)\n"
"       (find-file (expand-file-name file \\\"$DIR\\\"))\n"
"       (org-babel-tangle)\n"
"       (kill-buffer)) '($FILES)))\" 2>&1 |grep tangled\n"
msgstr ""
"emacs -Q --batch -l $ORGINSTALL \\\n"
"--eval \"(progn\n"
"(add-to-list 'load-path (expand-file-name \\\"~/src/org/lisp/\\\"))\n"
"(add-to-list 'load-path (expand-file-name \\\"~/src/org/contrib/lisp/\\\"))\n"
"(require 'org)(require 'org-exp)(require 'ob)(require 'ob-tangle)\n"
"(mapc (lambda (file)\n"
"       (find-file (expand-file-name file \\\"$DIR\\\"))\n"
"       (org-babel-tangle)\n"
"       (kill-buffer)) '($FILES)))\" 2>&1 |grep tangled\n"

