msgstr ""

#. type: item
#: org.texi:6393
#, no-wrap
msgid ":prepend"
msgstr ""

#+glossary: information ( 情報 ) / line ( 行|ライン|線 ) / list ( リスト ) / property ( プロパティ ) / setting ( 設定 ) / table ( テーブル )
#. type: table
#: org.texi:6397
