#. type: itemize
#: org.texi:12463
msgid "If an input table looks like it has column names (because its second row is an hline), then the column names will be removed from the table before processing, then reapplied to the results."
msgstr ""

#. type: example
#: org.texi:12470
#, no-wrap
msgid ""
"#+tblname: less-cols\n"
"| a |\n"
"|---|\n"
"| b |\n"
"| c |\n"
"\n"
msgstr ""

