#. type: cindex
#: org.texi:11255
#, no-wrap
msgid "code block, structure"
msgstr ""

#+glossary: code block ( コードブロック ) / source ( ソース ) / structure ( 構造 )
#. type: cindex
#: org.texi:11256
#, no-wrap
msgid "source code, block structure"
msgstr ""

#+glossary: code block ( コードブロック ) / structure ( 構造 )
#. type: Plain text
#: org.texi:11259
msgid "The structure of code blocks is as follows:"
msgstr ""

#. type: example
#: org.texi:11265
#, no-wrap
msgid ""
"#+srcname: <name>\n"
"#+begin_src <language> <switches> <header arguments>\n"
"  <body>\n"
"#+end_src\n"
msgstr ""

#+glossary: header argument ( ヘッダー引数 ) / code ( コード ) / embedded ( 組み込み ) / inline ( インライン ) / text ( テキスト|文章 )
#. type: Plain text
#: org.texi:11269
msgid "Switches and header arguments are optional. Code can also be embedded in text inline using"
msgstr ""

#. type: example
#: org.texi:11272
#, no-wrap
msgid "src_<language>@{<body>@}\n"
msgstr ""

#. type: Plain text
#: org.texi:11275
msgid "or"
msgstr ""

#. type: example
#: org.texi:11278
#, no-wrap
msgid "src_<language>[<header arguments>]@{<body>@}\n"
