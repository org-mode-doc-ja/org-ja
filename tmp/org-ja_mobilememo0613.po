#. type: code{#1}
#: org.texi:12496
#, no-wrap
msgid ":rownames"
msgstr ""

#+glossary: header argument ( ヘッダー引数 ) / default ( デフォルト|初期値 ) / value ( 値 )
#. type: Plain text
#: org.texi:12500
msgid "The @code{:rownames} header argument can take on the values @code{yes} or @code{no}, with a default value of @code{no}."
msgstr ""

#+glossary: row ( 行 )
#. type: itemize
#: org.texi:12504
msgid "No row name pre-processing will take place."
msgstr ""

#+glossary: column ( 列 ) / processing ( 処理 ) / table ( テーブル )
#. type: itemize
#: org.texi:12508
msgid "The first column of the table is removed from the table before processing, and is then reapplied to the results."
