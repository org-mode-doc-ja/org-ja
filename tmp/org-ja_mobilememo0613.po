msgstr "work/!-WAITING-NEXT"

#+glossary: line ( 行|ライン|線 )
#. type: table
#: org.texi:7387
msgid "Select @samp{:work:}-tagged TODO lines that are neither @samp{WAITING} nor @samp{NEXT}"
msgstr "@samp{:work:}を選択。ただしTODO行では@samp{WAITING}と@samp{NEXT}のどちらのタグもついていないもの"

#. type: item
#: org.texi:7387
#, no-wrap
msgid "work/!+WAITING|+NEXT"
msgstr "work/!+WAITING|+NEXT"

#+glossary: line ( 行|ライン|線 )
#. type: table
#: org.texi:7390
msgid "Select @samp{:work:}-tagged TODO lines that are either @samp{WAITING} or @samp{NEXT}."
msgstr "@samp{:work:}を選択。TODO行に@samp{WAITING}か@samp{NEXT}かどちらかのタグがついているもの。"

#+glossary: file ( ファイル ) / timeline ( タイムライン )
#. type: subsection
#: org.texi:7393
#, no-wrap
msgid "Timeline for a single file"
msgstr "Timeline for a single file"

