#. type: example
#: org.texi:12475
#, no-wrap
msgid ""
"#+srcname: echo-table-again\n"
"#+begin_src python :var tab=less-cols\n"
"  return [[val + '*' for val in row] for row in tab]\n"
"#+end_src\n"
"\n"
msgstr ""

#. type: example
#: org.texi:12481
#, no-wrap
msgid ""
"#+results: echo-table-again\n"
"| a  |\n"
"|----|\n"
"| b* |\n"
"| c* |\n"
